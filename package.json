{
  "name": "tp-virtual-list",
  "version": "0.1.4",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/tp-virtual-list.umd.js",
  "module": "./dist/tp-virtual-list.es.js",
  "exports": {
    "./dist/": "./dist/",
    ".": {
      "import": "./dist/tp-virtual-list.es.js",
      "require": "./dist/tp-virtual-list.umd.js"
    }
  },
  "keywords": [
    "vue",
    "vue3",
    "virtual-list",
    "list",
    "scroll",
    "infinite",
    "虚拟滚动",
    "虚拟列表"
  ],
  "description": "Vue.js 3.x 虚拟列表插件",
  "repository": {
    "type": "git",
    "url": "https://github.com/topul/virtual-list.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "tp-virtual-list": "^0.1.3",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
